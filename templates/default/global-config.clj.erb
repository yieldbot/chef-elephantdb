{ :replication <%= @replication %>
  :hosts [<% @cluster_nodes.each do |edb_node| %>"<%= edb_node.private_ip %>" <% end %>]
  :port <%= node['elephantdb']['port'] %>
  :domains {
    <% @node['elephantdb']['domains'].each do |domain| %>
      "<%= domain %>" "<%= @node['elephantdb']['domain_prefix']%>/<%= domain %>"
    <% end %>
  }
}
